/dts-v1/;
/ {
	description = "FIT source file for rockchip AMP";
	#address-cells = <1>;

	images {

		amp0 {
			description  = "bare-mental-core0";
			data         = /incbin/("rtt0.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x000>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x04000000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp1 {
			description  = "bare-mental-core1";
			data         = /incbin/("rtt1.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x100>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x00800000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp2 {
			description  = "bare-mental-core2";
			data         = /incbin/("rtt2.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x200>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x01000000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp3 {
			description  = "bare-mental-core3";
			data         = /incbin/("rtt3.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x300>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x01800000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp4 {
			description  = "bare-mental-core4";
			data         = /incbin/("rtt4.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x400>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x02000000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp5 {
			description  = "bare-mental-core5";
			data         = /incbin/("rtt5.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x500>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x02800000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
			udelay       = <10000>;
			hash {
				algo = "sha256";
			};
		};

		amp6 {
			description  = "bare-mental-core6";
			data         = /incbin/("rtt6.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x600>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x03000000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;

				algo = "sha256";
			};
		};

		amp7 {
			description  = "bare-mental-core7";
			data         = /incbin/("rtt7.bin");
			type         = "firmware";
			compression  = "none";
			arch         = "arm64";	 // "arm64" or "arm"
			cpu          = <0x700>;  // mpidr
			thumb        = <0>;      // 0: arm or thumb2; 1: thumb
			hyp          = <0>;      // 0: el1/svc; 1: el2/hyp
			load         = <0x03800000>;
			size         = <0x00800000>;
			srambase     = <0xff000000>;
			sramsize     = <0x00100000>;
